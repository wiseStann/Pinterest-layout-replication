<div class="feed__layout-container">
	{{#each pins}}
		<div id="pin__content-container-{{this.PinID}}">
			<img class="pin__image-{{this.PinID}}" src="{{this.MediaUrl}}">
			<div class="pin__author_container">
				<img class="pin__author_avatar" src="{{this.AuthorAvatar}}">
				<p>{{{this.AuthorName}}}</p>
			</div>
		</div>
	{{/each}}
</div>
